pipeline {
   agent any 

     tools {
       maven 'M2_HOME'
      }
     stages {
       steps('Checkout') {
         echo 'Checkout the source code from GitHub'
           git branch: 'master', url:  'https://github.com/Garimabhati/Insurance-project-17May.git'
             }
       }
     stages('Package') {
        steps {
          echo 'Create a package'
           sh 'mvn clean package'
              }
      }
   }
}
