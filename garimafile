pipeline {
  agent any
  tools {
    maven 'M2_HOME'
  }
  stages {
    stage('Git checkout') {
      steps {
        echo 'This is for cloning the git repo'
        git branch: 'master', url: 'https://github.com/Garimabhati/Insurance-project-17May.git'
      }
    }
    stage('Create a Package') {
      steps {
        echo 'This will create a package using Maven'
        sh 'mvn package'
      }
    }
  }
}

